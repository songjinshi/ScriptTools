<?xml version="1.0" encoding="utf-8"?>
<manifest android:versionCode="1" android:versionName="1.0" package="com.mediatek.appguide.plugin"
  xmlns:android="http://schemas.android.com/apk/res/android">
    <application android:label="AppGuidePlugin">
        <uses-library android:name="com.google.android.media.effects" android:required="false" />
        <service android:name=".MmsGuidePlugin" android:exported="true">
            <intent-filter>
                <action android:name="com.mediatek.mms.ext.IAppGuideExt" />
            </intent-filter>
            <meta-data android:name="class" android:value="com.mediatek.appguide.plugin.mms.SwitchSimMmsExt" />
        </service>
        <service android:name=".ContactsGuidePlugin" android:exported="true">
            <intent-filter>
                <action android:name="com.android.contacts.ext.IContactPlugin" />
            </intent-filter>
            <meta-data android:name="class" android:value="com.mediatek.appguide.plugin.contacts.ContactsPlugin" />
        </service>
        <service android:name=".CameraGuidePlugin" android:exported="true">
            <intent-filter>
                <action android:name="com.mediatek.camera.ext.IAppGuideExt" />
            </intent-filter>
            <meta-data android:name="class" android:value="com.mediatek.appguide.plugin.camera.ScrollAndZoomExt" />
        </service>
    </application>
</manifest>