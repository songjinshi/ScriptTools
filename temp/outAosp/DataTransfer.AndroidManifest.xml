        </activity>
        <activity android:label="@string/backup_personal_data" android:name=".PersonalDataRestoreActivity" android:launchMode="singleTask" android:configChanges="keyboardHidden|orientation|screenSize">
            <intent-filter>
                <action android:name="com.mediatek.backuprestore.intent.PersonalDataRestoreActivity" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:label="@string/backup_app" android:icon="@drawable/ic_application" android:name="com.mediatek.datatransfer.AppRestoreActivity" android:launchMode="singleTask" android:configChanges="keyboardHidden|orientation|screenSize">
