    <application android:label="@string/app_name" android:icon="@drawable/ic_backuprestore_launcher" android:uiOptions="splitActionBarWhenNarrow">
        <activity android:theme="@*android:style/Theme.Black.NoTitleBar" android:name=".BootActivity" android:launchMode="singleInstance" android:configChanges="keyboardHidden|orientation|screenSize">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:label="@string/app_name" android:icon="@drawable/ic_backuprestore_launcher" android:name=".MainActivity" android:configChanges="keyboardHidden|navigation|orientation" android:uiOptions="splitActionBarWhenNarrow">
            <intent-filter>
                <action android:name="com.mediatek.backuprestore.intent.MainActivity" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".BackupMainActivity" android:configChanges="keyboardHidden|orientation|screenSize" android:uiOptions="splitActionBarWhenNarrow" />
        <activity android:name=".RestoreMainActivity" android:configChanges="keyboardHidden|orientation|screenSize" android:uiOptions="splitActionBarWhenNarrow" />
        <activity android:label="@string/backup_personal_data" android:name=".PersonalDataBackupActivity" android:launchMode="singleTask" android:configChanges="keyboardHidden|orientation|screenSize">
