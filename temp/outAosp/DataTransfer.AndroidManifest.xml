                <action android:name="com.mediatek.backuprestore.intent.PersonalDataBackupActivity" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:label="@string/backup_app" android:icon="@drawable/ic_application" android:name=".AppBackupActivity" android:launchMode="singleTask" android:configChanges="keyboardHidden|orientation|screenSize">
