        <service android:label="@string/wallpaper_magicsmoke" android:name="MagicSmoke" android:permission="android.permission.BIND_WALLPAPER">
            <intent-filter>
                <action android:name="android.service.wallpaper.WallpaperService" />
            </intent-filter>
            <meta-data android:name="android.service.wallpaper" android:resource="@xml/magicsmoke" />
        </service>
        <activity android:theme="@style/Preview" android:name="com.android.magicsmoke.MagicSmokeSelector" android:exported="true" android:configChanges="orientation|screenSize" />
    </application>
</manifest>