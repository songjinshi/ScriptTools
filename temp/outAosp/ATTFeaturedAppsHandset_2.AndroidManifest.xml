<?xml version="1.0" encoding="utf-8"?>
<manifest android:versionCode="256" android:versionName="2.0.47.256" package="com.att.featuredapps.handset"
  xmlns:android="http://schemas.android.com/apk/res/android">
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <application android:label="@string/app_name" android:icon="@drawable/ic_launcher" android:debuggable="false" android:allowBackup="true">
        <receiver android:name=".HandsetWidgetProvider">
            <intent-filter>
                <action android:name="android.appwidget.action.APPWIDGET_UPDATE" />
                <action android:name="action.featured.apps.arrow.left" />
                <action android:name="action.featured.apps.arrow.right" />
                <action android:name="action.featured.apps.auto.flip" />
                <action android:name="action.featured.apps.layout.clear" />
                <action android:name="action.featured.apps.eula.accepted" />
                <action android:name="action.featured.apps.widget.update" />
                <action android:name="action.featured.apps.widget.update_content" />
                <action android:name="action.featured.apps.item.click" />
                <action android:name="action.featured.apps.flipper.lock" />
            </intent-filter>
            <meta-data android:name="android.appwidget.provider" android:resource="@xml/widget" />
        </receiver>
        <service android:name=".services.UpdateContentService" android:exported="false">
            <intent-filter>
                <action android:name="action.featured.apps.do_init_content" />
                <action android:name="action.featured.apps.do_update_content" />
                <action android:name="action.featured.apps.update_content.reload" />
            </intent-filter>
        </service>
        <activity android:theme="@*android:style/Theme.Light.NoTitleBar" android:name=".EulaActivity" android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.appwidget.action.APPWIDGET_CONFIGURE" />
            </intent-filter>
        </activity>
        <activity android:theme="@*android:style/Theme.Light.NoTitleBar" android:name=".EulaFullActivity" android:screenOrientation="portrait" />
        <receiver android:name=".localytics.StatisticsProvider">
            <intent-filter>
                <action android:name="action.featured.apps.statistics.send" />
            </intent-filter>
        </receiver>
    </application>
</manifest>